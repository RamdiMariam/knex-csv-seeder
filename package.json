{
  "name": "knex-csv-seeder",
  "version": "0.2.3",
  "description": "CSV file seeder for Knex",
  "main": "lib/seeder.js",
  "files": [
    "lib/*.js",
    "README.md"
  ],
  "scripts": {
    "test": "multi='dot=- mocha-lcov-reporter=lcov-report.lcov' ./node_modules/.bin/mocha test",
    "build": "./node_modules/.bin/babel -d lib -s inline src",
    "watch": "./node_modules/.bin/babel -d lib -s inline -w src"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/holyshared/knex-csv-seeder.git"
  },
  "author": "Noritaka Horio",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/holyshared/knex-csv-seeder/issues"
  },
  "homepage": "https://github.com/holyshared/knex-csv-seeder",
  "dependencies": {
    "bluebird": "^3.0.5",
    "csv-parse": "^1.0.0",
    "iconv-lite": "^0.4.13",
    "knex": "^0.9.0"
  },
  "devDependencies": {
    "babel-cli": "^6.1.2",
    "babel-preset-es2015": "^6.1.2",
    "blanket": "^1.1.9",
    "codecov": "^1.0.1",
    "coffee-script": "^1.10.0",
    "eslint": "^1.8.0",
    "espower-coffee": "^1.0.0",
    "mocha": "^2.3.3",
    "mocha-lcov-reporter": "^1.0.0",
    "mocha-multi": "^0.7.2",
    "mysql": "^2.9.0",
    "power-assert": "^1.1.0"
  },
  "config": {
    "blanket": {
      "pattern": "/lib/",
      "data-cover-never": "/node_modules/"
    }
  }
}
